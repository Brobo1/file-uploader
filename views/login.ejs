<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
    <link
            rel="stylesheet"
            href="/css/style.css"
    >
</head>
<body>
<%- include("partials/header.ejs") %>
<div id="container">
    <form
            action="/login"
            method="post"
            id="login-form"
    >
        <div>
            <label for="username"></label>
            <input
                    type="text"
                    name="username"
                    id="username"
                    placeholder="Username"
            >
        </div>
        <div>
            <label for="password"></label>
            <input
                    type="password"
                    name="password"
                    id="password"
                    placeholder="Password"
            >
        </div>
        <button type="submit">Login</button>
    </form>
</div>

</body>
</html>