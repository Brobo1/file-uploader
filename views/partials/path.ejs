<div id="folder-path" class="flex gap-x-2 flex-wrap" >
    <% path.forEach((path) => { %>
        <div class="flex gap-x-2  text-xl whitespace-nowrap">
            <a href="/folder/<%= path.id %>" class="hover:text-blue-500" data-id="load">
                <%= path.name === "root" ? "Home" : path.name %>
            </a>
                <p class="font-light">/</p>
        </div>
    <% }) %>
</div>